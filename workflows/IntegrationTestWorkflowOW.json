        "input3": "input3.txt",
        "input4": "input4.txt"
      },
      "InvokeNext": ["test-py-api", "test-r-api"],
      "FaaSServer": "GH",
      "Type": "Python",
      "FunctionName": "create_input"
    },
    "test-py-api": {
      "Arguments": {
        "folder": "integration-tests",
        "input1": "input1.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "output1": "output1-py.txt",
        "output2": "output2-py.txt"
      },
      "InvokeNext": ["sync1"],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_py_api"
    },
    "test-r-api": {
      "Arguments": {
        "folder": "integration-tests",
        "input4": "input4.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "output1": "output1-R.txt",
        "output2": "output2-R.txt"
      },
      "InvokeNext": ["sync1"],
      "FaaSServer": "OW",
      "Type": "R",
      "FunctionName": "test_r_api"
    },
    "sync1": {
      "Arguments": {
        "folder": "integration-tests",
        "input1": "input1.txt",
        "input2": "input2.txt",
        "input3": "input3.txt",
        "input4": "input4.txt",
        "output1_py": "output1-py.txt",
        "output2_py": "output2-py.txt",
        "output1_R": "output1-R.txt",
        "output2_R": "output2-R.txt"
      },
      "InvokeNext": [
        {
          "True": ["test-run-true"],
          "False": ["test-dontrun-false"]
        }
      ],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "sync1"
    },
    "test-run-true": {
      "Arguments": {
        "folder": "integration-tests",
        "output": "run_true_output.txt"
      },
      "InvokeNext": [
        {
          "True": ["test-dontrun-true"],
          "False": ["test-run-false"]
        }
      ],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_run_true"
    },
    "test-dontrun-false": {
      "InvokeNext": [],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_dontrun_false"
    },
    "test-run-false": {
      "Arguments": {
        "folder": "integration-tests",
        "output": "run_false_output.txt"
      },
      "InvokeNext": ["test-rank(5)"],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_run_false"
    },
    "test-dontrun-true": {
      "InvokeNext": [],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_dontrun_true"
    },
    "test-rank": {
      "Arguments": {
        "folder": "integration-tests",
        "rank_folder": "rank_files"
      },
      "InvokeNext": ["sync2"],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "test_rank"
    },
    "sync2": {
      "Arguments": {
        "folder": "integration-tests",
        "rank_folder": "rank_files",
        "run_true_output": "run_true_output.txt",
        "run_false_output": "run_false_output.txt"
      },
      "InvokeNext": [],
      "FaaSServer": "OW",
      "Type": "Python",
      "FunctionName": "sync2"
    }
